{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@views/*": ["views/*"],
      "@viewmodels/*": ["viewmodels/*"],
      "@services/*": ["services/*"],
      "@components/*": ["components/*"],
      "@models/*": ["models/*"]
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
